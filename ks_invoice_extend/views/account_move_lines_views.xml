<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_move_form_inherit_delivered_work" model="ir.ui.view">
        <field name="name">account.move.form.inherit.delivered.work</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='account_id']" position="after">
                <field name="delivered_work" optional="hide"/>
            </xpath>
        </field>
    </record>
</odoo>